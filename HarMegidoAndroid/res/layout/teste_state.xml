<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:opencv="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    

   	<Button
       android:id="@+id/btnListarPartidas"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="Listar Partidas" />

   
       <Button
       android:id="@+id/btnCriarPartida"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="Criar Partida" />
       
       <Button
       android:id="@+id/btnEntrarPartida"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="Entrar na Partida" />
       
       <Button
       android:id="@+id/btnSairPartida"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="Sair da Partida" />
       
        <Button
       android:id="@+id/btnEncontrarPartida"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="Encontrar a Partida" />
        
       <Button
       android:id="@+id/btnListarJogadores"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="Listar Jogadores" />
       
       <Button
       android:id="@+id/btnMudarTime"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="Mudar Time" />
        
        <Button
       android:id="@+id/btnEncantar"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="Encantar" />
        
        <Button
       android:id="@+id/btnListarEncantamentos"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="Listar Encantamentos" />
        
         <Button
       android:id="@+id/btnDesencantar"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="Desencantar" />
         
         <Button
       android:id="@+id/btnListarDesencamentos"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="Listar Desencantamentos" />
         
       <Button
       android:id="@+id/btnVerPontuacao"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="Ver Pontuacao" />
         
       <TextView
       android:id="@+id/txtViewRetorno"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="TextView" />

</LinearLayout>